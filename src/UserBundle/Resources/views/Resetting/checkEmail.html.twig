{% extends "AppBundle::base.html.twig" %}
{% trans_default_domain 'AppBundle' %}

{% block content %}
<div class="container content traveller-registration">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <h1>{{ 'breadcrumbs.fos_user_resetting_check_email'|trans }}</h1>
                </div>
            </div>
            <p>{{ 'resetting.check_email'|trans({'%email%': email}, 'FOSUserBundle') }}</p>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 banner-xs">
            {% include 'AppBundle:blocks:banners.html.twig' %}
        </div>
    </div>
</div>
{% endblock %}
